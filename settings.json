                "react-dom",
                "react-router",
                "react-router-redux",
                "react-redux",
                "react-cookie",
                "react-bootstrap/lib/",
                "react-bootstrap-typeahead",
                "react-day-picker",
                "react-loader",
                "redux",
                "redux-thunk",
                "redux-logger",
                "redux-notifications",
                "redux-form",
