{
    "general": {
        "DEV_SERVER_PORT": 8085,
        "REST_API_URL": "http://localhost:8085",
        "DEV_SERVER_BUILD_PATH": "build"
    },
    "applications": [
        {
            "name": "corp-app",
            "rootPath": "corp-app",
            "path": "corp-app/index.js",
            "vendor": [
                "lodash",
